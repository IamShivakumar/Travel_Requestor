{
  "builds": [
    {
      "src": "main/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "pip": ["psycopg2-binary"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main/wsgi.py"
    }
  ]
}