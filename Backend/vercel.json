{
    "builds": [
      {
        "src": "main/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12",
          "pip": {
            "install": [
              "-r",
              "requirements.txt"
            ]
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main/wsgi.py"
      }
    ]
  }